study learn
#define _CRT_SECURE_NO_WARNINGS 1
#include<stdio.h>
#include<time.h>
#include<string.h>

int main()
{
	int a = 0;
	int b = 0;
	int c = 0;
	int x = 0;
	printf("输入3个数字:");
	scanf("%d%d%d", &a, &b, &c);
	if (a < b)
	{
		x = a;
		a = b;
		b = x;
	}
	if (a < c)
	{
		x = a;
		a = c;
		c = x;
	}
	if (b < c)
	{
		x = b;
		b = c;
		c = x;
	}
	printf("%d %d %d", a, b, c);
	return 0;
}
