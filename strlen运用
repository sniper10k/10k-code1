study learn
#define _CRT_SECURE_NO_WARNINGS 1
#include<stdio.h>
#include<math.h>

int main()
{
	char arr1[] = "#####################";
	char arr2[] = "welcome to bir!!!!!!!";
	int left = 0;
	int right = strlen(arr1)-1;
	while (left <= right)
	{
		arr1[left] = arr2[left];
		arr1[right] = arr2[right];
		printf("%s\n", arr1);
		Sleep(300);
		system("cls");
		left++;
		right--;
	}
	printf("%s\n", arr1);
	return 0;
}
